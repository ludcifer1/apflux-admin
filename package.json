{
	"name": "metronic-angular",
	"version": "6.0.4",
	"license": "MIT",
	"scripts": {
		"ng": "ng",
		"build": "npm run env -s && ng build --prod",
		"build-dev": "npm run env -s && ng build --prod --c=dev",
		"build-prod": "npm run env -s && ng build --prod --c=prod",
		"build-uat": "npm run env -s && ng build --prod --c=uat",
		"start": "ng serve --aot",
		"lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
		"test": "npm run env -s && ng test",
		"test:ci": "npm run env -s && npm run lint -s && ng test --code-coverage --watch=false",
		"e2e": "npm run env -s && ng e2e",
		"bundle-report": "webpack-bundle-analyzer dist/metronic/stats.json"
	},
